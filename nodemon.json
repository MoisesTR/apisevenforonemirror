{
  "delay": "1500",
  "verbose": true,
  "watch": [
    "src/**/*.ts",
    "config"
  ],
  "ignore": [
    "dist",
    "build",
    "src/logs",
    "src/uploads/"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js json ts",
  "exec": "ts-node --project ./tsconfig.json  --files ./src/index.ts"
}
